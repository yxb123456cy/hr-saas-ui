    "pinia": "^3.0.4",
    "pinia-plugin-persist": "^1.0.0",
    "tdesign-icons-vue-next": "^0.4.1",
    "tdesign-vue-next": "^1.17.7",
    "vue": "^3.5.25",
    "vue-echarts": "^8.0.1",
    "vue-element-plus-x": "^1.3.98",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3",
    "vue3-scroll-seamless": "^1.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
