{
  "name": "hr-saas-ui",
  "version": "0.0.0",
  "author": "@yxb123456",
  "main": "src/main.ts",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "web:dev": "vite --mode dev",
    "web:dev:test": "vite --mode testing",
    "web:dev:prod": "vite --mode prod",
    "web:build": "run-p type-check \"build-only {@}\" --",
    "web:build:dev": "pnpm web:build -- --mode dev",
    "web:build:test": "pnpm web:build -- --mode testing",
    "web:build:prod": "pnpm web:build -- --mode prod",
    "web:preview": "vite preview --mode prod",
    "web:test:unit": "vitest",
    "web:test:e2e": "playwright test",
    "web:docker:dev": "",
    "web:docker:test": "",
    "web:docker:build": "",
    "web:compose:dev": "",
    "web:compose:test": "",
    "web:compose:build": "",
    "build-only": "vite build",
    "clean": "rimraf node_modules",
    "commit": "git-cz",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "release:changelog:major": "standard-version --release-as major",
    "release:changelog:minor": "standard-version --release-as minor",
    "release:changelog:patch": "standard-version --release-as patch",
    "prepare": "husky"
  },
  "dependencies": {
